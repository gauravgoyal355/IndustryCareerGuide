{
  "metadata": {
    "version": "1.0",
    "description": "Detailed career progression paths with milestones, timelines, and actionable steps",
    "lastUpdated": "2025-01-20"
  },
  "trajectories": {
    "data_scientist": {
      "phases": {
        "preparation": {
          "duration": "3-6 months",
          "description": "Build foundational skills and portfolio",
          "milestones": [
            {
              "title": "Complete Python/R Programming Course",
              "timeframe": "Month 1-2",
              "priority": "high",
              "tasks": [
                "Complete Python for Data Science course",
                "Build 2-3 data analysis projects",
                "Learn pandas, numpy, matplotlib libraries",
                "Create GitHub portfolio"
              ],
              "success_criteria": "Comfortable manipulating data and creating visualizations"
            },
            {
              "title": "Master Statistical Foundations",
              "timeframe": "Month 2-3",
              "priority": "high",
              "tasks": [
                "Study descriptive and inferential statistics",
                "Learn hypothesis testing",
                "Understand regression analysis",
                "Practice with real datasets"
              ],
              "success_criteria": "Can explain statistical concepts and apply them to data problems"
            },
            {
              "title": "Build Portfolio Projects",
              "timeframe": "Month 3-6",
              "priority": "medium",
              "tasks": [
                "Complete 3-5 end-to-end data science projects",
                "Include projects from different domains",
                "Document projects clearly on GitHub",
                "Present findings in blog posts or presentations"
              ],
              "success_criteria": "Strong portfolio demonstrating technical and communication skills"
            }
          ]
        },
        "job_search": {
          "duration": "2-4 months",
          "description": "Active job searching and interview preparation",
          "milestones": [
            {
              "title": "Resume and Application Materials",
              "timeframe": "Month 1",
              "priority": "high",
              "tasks": [
                "Create data science-focused resume",
                "Write compelling cover letter template",
                "Optimize LinkedIn profile",
                "Prepare portfolio website"
              ],
              "success_criteria": "Professional materials that highlight quantitative and technical skills"
            },
            {
              "title": "Technical Interview Preparation",
              "timeframe": "Month 1-3",
              "priority": "high",
              "tasks": [
                "Practice coding challenges (Python/SQL)",
                "Review machine learning concepts",
                "Prepare to discuss portfolio projects",
                "Practice explaining technical concepts simply"
              ],
              "success_criteria": "Confident in technical interviews and project discussions"
            },
            {
              "title": "Apply and Network",
              "timeframe": "Month 1-4",
              "priority": "medium",
              "tasks": [
                "Apply to 10-15 positions per week",
                "Attend data science meetups and conferences",
                "Connect with data scientists on LinkedIn",
                "Participate in online data science communities"
              ],
              "success_criteria": "Strong pipeline of opportunities and professional network"
            }
          ]
        },
        "first_role": {
          "duration": "12-18 months",
          "description": "Establish yourself in your first data science role",
          "milestones": [
            {
              "title": "Quick Wins and Learning",
              "timeframe": "Month 1-6",
              "priority": "high",
              "tasks": [
                "Deliver on first project assignments",
                "Learn company-specific tools and data",
                "Build relationships with stakeholders",
                "Seek feedback and mentorship"
              ],
              "success_criteria": "Positive performance reviews and growing responsibilities"
            },
            {
              "title": "Advanced Skill Development",
              "timeframe": "Month 6-18",
              "priority": "medium",
              "tasks": [
                "Learn advanced machine learning techniques",
                "Develop business domain expertise",
                "Lead a small project or analysis",
                "Present findings to senior leadership"
              ],
              "success_criteria": "Recognition as a competent data scientist ready for more responsibility"
            }
          ]
        },
        "career_growth": {
          "duration": "2-5 years",
          "description": "Advance to senior individual contributor or management roles",
          "milestones": [
            {
              "title": "Senior Data Scientist",
              "timeframe": "Year 2-3",
              "priority": "high",
              "tasks": [
                "Lead cross-functional projects",
                "Mentor junior data scientists",
                "Develop expertise in specialized areas",
                "Drive strategic data initiatives"
              ],
              "success_criteria": "Promotion to senior role with increased scope and impact"
            },
            {
              "title": "Specialization or Leadership",
              "timeframe": "Year 3-5",
              "priority": "medium",
              "tasks": [
                "Choose between management or IC track",
                "Build external reputation through speaking/writing",
                "Lead major data science initiatives",
                "Contribute to hiring and team building"
              ],
              "success_criteria": "Clear career trajectory as either technical leader or people manager"
            }
          ]
        }
      },
      "skills_timeline": {
        "immediate": ["Python programming", "Statistics", "Data manipulation", "SQL"],
        "short_term": ["Machine learning", "Data visualization", "Business communication", "Git/GitHub"],
        "medium_term": ["Advanced ML", "Big data tools", "A/B testing", "Product analytics"],
        "long_term": ["Strategic thinking", "Team leadership", "Domain expertise", "Mentoring"]
      },
      "common_challenges": [
        {
          "challenge": "Translating academic research to business impact",
          "solution": "Focus on learning business context and measuring impact in business terms"
        },
        {
          "challenge": "Keeping up with rapidly evolving technology",
          "solution": "Dedicate time for continuous learning and participate in data science communities"
        },
        {
          "challenge": "Communicating technical results to non-technical stakeholders",
          "solution": "Practice storytelling with data and focus on business implications"
        }
      ]
    },
    "research_scientist": {
      "phases": {
        "preparation": {
          "duration": "2-4 months",
          "description": "Transition from academic to industry research mindset",
          "milestones": [
            {
              "title": "Industry Research Orientation",
              "timeframe": "Month 1-2",
              "priority": "high",
              "tasks": [
                "Study industry research practices",
                "Understand intellectual property basics",
                "Learn about product development cycles",
                "Network with industry researchers"
              ],
              "success_criteria": "Clear understanding of industry vs academic research differences"
            },
            {
              "title": "Technical Portfolio Development",
              "timeframe": "Month 1-4",
              "priority": "medium",
              "tasks": [
                "Highlight applied research projects",
                "Emphasize practical applications",
                "Document collaboration experiences",
                "Showcase interdisciplinary work"
              ],
              "success_criteria": "Portfolio demonstrating industry-relevant research skills"
            }
          ]
        },
        "job_search": {
          "duration": "3-6 months",
          "description": "Target industry research positions",
          "milestones": [
            {
              "title": "Industry Research Network",
              "timeframe": "Month 1-3",
              "priority": "high",
              "tasks": [
                "Attend industry conferences",
                "Connect with corporate researchers",
                "Join professional associations",
                "Reach out to PhD alumni in industry"
              ],
              "success_criteria": "Strong network of industry research contacts"
            },
            {
              "title": "Application Strategy",
              "timeframe": "Month 2-6",
              "priority": "high",
              "tasks": [
                "Target R&D labs at major corporations",
                "Apply to research-focused startups",
                "Consider national labs and institutes",
                "Prepare for technical presentations"
              ],
              "success_criteria": "Multiple interview opportunities in target companies"
            }
          ]
        }
      },
      "skills_timeline": {
        "immediate": ["Research methodology", "Technical writing", "Data analysis"],
        "short_term": ["IP awareness", "Industry collaboration", "Project management"],
        "medium_term": ["Technology transfer", "Business applications", "Cross-functional work"],
        "long_term": ["Research strategy", "Team leadership", "Innovation management"]
      }
    }
  },
  "transition_maps": {
    "academic_to_industry": {
      "common_paths": [
        {
          "from": "PhD/Postdoc",
          "to": "data_scientist",
          "typical_duration": "6-12 months",
          "success_rate": "75%",
          "key_differentiators": ["Programming skills", "Business communication", "Applied projects"]
        },
        {
          "from": "PhD/Postdoc",
          "to": "research_scientist",
          "typical_duration": "3-6 months",
          "success_rate": "60%",
          "key_differentiators": ["Industry research experience", "Applied focus", "IP awareness"]
        }
      ]
    }
  }
}
